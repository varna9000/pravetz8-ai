0 REM ПРОТОТИП НА ИНТЕРФЕЙСНА ИИ ПРОГРАМА ЗА ПРАВЕЦ 8

1 M$ = "ATC" + CHR$(34) + "192.168.3.127:6502" + CHR$(34) + CHR$(10)
2 N$ = "ATQ1"
4 P$="" : B=0
5 INPUT "NEW CONNECTION (Y/N): ";Q$
6 IF Q$ = "N" GOTO 40

10 POKE $C50B, $18
20 POKE $C50A, $0B
35 GOSUB 400

40 INPUT A$
50 A$ = "ATT"+ CHR$(34) + A$ + CHR$(34) + CHR$(10)
60 GOSUB 200

100 R=PEEK($C508)
105 IF R=B GOTO 100
106 R=B
107 P$=P$+CHR$(R)
110 IF R=10 GOTO 160
120 GOTO 40
199 END

200 FOR I = 1 TO LEN(A$)
210 CH$ = MID$(A$, I, 1)
220 POKE $C5008, ASC(CH$)
230 NEXT I
240 RETURN

400 FOR I = 1 TO LEN(M$)
410 CH$ = MID$(M$, I, 1)
420 POKE $C5008, ASC(CH$)
430 NEXT I
431 PRINT M$
440 RETURN
